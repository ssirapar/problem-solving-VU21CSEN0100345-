#include<stdio.h>
int fibbo(int r)
{
    if (r==0)
    {
    return 0;    
    }
    else if(r==1)
    {
        return 1;
    }
    else
    {
        return fibbo(r-1)+fibbo(r-2);
    }
}
int main()
{
    int a,b;
    printf("enter the number of outputs you want");
    scanf("%d",&b);
    for(a=0;a<=b;a++)
    {
        printf("%d",fibbo(a));
    }
}

