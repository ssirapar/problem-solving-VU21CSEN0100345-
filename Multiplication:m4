//5.Multiplication:
#include"stdio.h"
int main()
{
int a[3][3],b[3][3],c[3][3],i,j;
printf("Enter the numbers in matrix A ");
for(i=1;i<=3;i++)
{
for(j=1;j<=3;j++)
{
scanf("%d",&a[i][j]);
}
}
printf("Enter the numbers in matrix b ");
for(i=1;i<=3;i++)
{
for(j=1;j<=3;j++)
{
scanf("%d",&b[i][j]);
}
}
for(i=1;i<=3;i++)
{
for(j=1;j<=3;j++)
{
c[i][j]=a[i][j]*b[i][j];
}
}
printf("\nThe result is\n");
for(i=1;i<=3;i++)
{
for(j=1;j<=3;j++)
{
printf("%5d",c[i][j]);
}
printf("\n");
}
return 0;
}

